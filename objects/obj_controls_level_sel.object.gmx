        <arguments>
          <argument>
            <kind>1</kind>
            <string>// controller input

ls = gamepad_axis_value(0, gp_axislh);
us = gamepad_axis_value(0, gp_axislv);

if(obj_globals.sec_left&gt;0){
    if(ls &lt; -0.9){
        level = 1;
        if(p1 == 0){
            audio_play_sound(snd_click, 10, false);
            p1 = 1;
            p2 = 0;
            p3 = 0;
        }
    }else if(ls &gt; 0.9){
        level = 3;
        if(p3 == 0){
            audio_play_sound(snd_click, 10, false);
            p1 = 0;
            p2 = 0;
            p3 = 1;
        }
    }
    if(us&lt;-0.9){
        level = 2;
        if(p2 == 0){
            audio_play_sound(snd_click, 10, false);
            p1 = 0;
            p2 = 1;
            p3 = 0;
        }
    }
}

show_debug_message('ls = ' + string(ls) + ' | us = ' + string(us));

if gamepad_button_check_pressed(0, gp_padl){ //  controller left
    if(obj_globals.sec_left&gt;0){
        level --;
        if(level == 0){
            level = 3;
        }
        audio_play_sound(snd_click, 10, false);
    }
}
if gamepad_button_check_pressed(0, gp_padr){ //  controller right
    if(obj_globals.sec_left&gt;0){
        level++;
        if(level == 4){
            level = 1;
        }
        audio_play_sound(snd_click, 10, false);
    }
}
if gamepad_button_check_pressed(0, gp_face1){ //  controller select
    if(obj_globals.sec_left&gt;0){
        if(level == 1){
        scr_screen_fade(rm_rocks);
        } else if(level == 2){
            scr_screen_fade(rm_stack);
        } else if(level == 3){
            scr_screen_fade(rm_slabs);
        }
        audio_play_sound(snd_sel, 10, false);
    }
}
if gamepad_button_check_pressed(0, gp_face2){ //  controller back
    if(obj_globals.sec_left&gt;0){
        scr_screen_fade(rm_menu);
        audio_play_sound(snd_back, 10, false);
    }
}

